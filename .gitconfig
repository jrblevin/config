[user]
	name = Jason Blevins
	email = jrblevin@sdf.org

[alias]
        br = branch
        st = status
        co = checkout
        ci = commit
        rlog = log --pretty=format:\"%h %Cblue%cr%Creset %cn %Cgreen%s%Creset\"
        lg = log -p
        df = diff
        dc = diff --cached

[color]
        ui = auto

[push]
	default = current

[core]
	excludesfile = ~/.gitignore
